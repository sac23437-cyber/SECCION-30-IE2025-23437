0002fd 9508                          RET
                                 
                                 INCREMENTAR_HORAS:
0002fe e000                          LDI R16, 0								// Reinicia las decenas de minutos
0002ff 9300 0103                     STS DECENAS_MINUTOS_RAM, R16			// Guardamos las decenas de minutos en RAM
                                 
000301 9100 0105                     LDS R16, DECENAS_HORAS_RAM				// Cargamos las decenas de horas desde RAM
000303 3002                          CPI R16, 2								// Verifica si el valor de las decenas de horas es 2
