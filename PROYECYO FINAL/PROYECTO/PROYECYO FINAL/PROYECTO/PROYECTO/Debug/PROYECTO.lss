                                     // Incrementa minutos
0002eb 9100 0102                     LDS R16, UNIDADES_MINUTOS_RAM			// Cargamos las unidades de minutos desde RAM
0002ed 3009                          CPI R16, 9								// Verificamos si el valor de las unidades de minutos es 9
0002ee f021                          BREQ INC_DECENAS_MINUTOS				// Si es 9, incrementamos las decenas de minutos
0002ef 9503                          INC R16									// Incrementa las unidades de minutos
0002f0 9300 0102                     STS UNIDADES_MINUTOS_RAM, R16			// Guardamos las unidades de minutos en RAM
0002f2 9508                          RET
                                 
                                 INC_DECENAS_MINUTOS:
0002f3 e000                          LDI R16, 0								// Reiniciamos las unidades de minutos
0002f4 9300 0102                     STS UNIDADES_MINUTOS_RAM, R16			// Guardamos las unidades de minutos en RAM
                                 
0002f6 9100 0103                     LDS R16, DECENAS_MINUTOS_RAM			// Cargamos las decenas de minutos desde RAM
0002f8 3005                          CPI R16, 5								// Verificamos si el valor de las decenas de minutos es 5
0002f9 f021                          BREQ INCREMENTAR_HORAS					// Si es 5, incrementamos las horas
