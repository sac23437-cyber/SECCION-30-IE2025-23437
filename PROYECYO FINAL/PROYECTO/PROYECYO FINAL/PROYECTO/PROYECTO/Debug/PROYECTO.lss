000295 9508                          RET
                                 //--------------------------------------------------------------------------------------------- COMPARAR ALARMA
                                 
                                 MOSTRAR_DIGITO:
                                     // Cargar el valor del dgito desde la tabla de 7 segmentos
000296 e0f0                          LDI ZH, HIGH(TABLA7SEG << 1)
000297 e4e4                          LDI ZL, LOW(TABLA7SEG << 1)
000298 0fe0                          ADD ZL, R16
000299 9104                          LPM R16, Z								// Obtenemos el valor de la tabla del display
00029a b90b                          OUT PORTD, R16							// Mostramos R16 en el display
00029b 9508                          RET
                                 
                                 DIVIDIR_DECENAS:
                                     // Divide un nmero en decenas (R16 / 10)
00029c e01a                          LDI R17, 10
00029d 2722                          CLR R18
                                 DIVIDE_LOOP:
00029e 9523                          INC R18
00029f 1b01                          SUB R16, R17
0002a0 f7e8                          BRCC DIVIDE_LOOP						// Si no hay acarreo, continuar
0002a1 952a                          DEC R18
0002a2 0f01                          ADD R16, R17
0002a3 2f02                          MOV R16, R18							// Guardamos las decenas en R16
0002a4 9508                          RET
                                 
                                 DIVIDIR_UNIDADES:
                                     // Obtiene las unidades de un nmero (R16 % 10)
0002a5 e01a                          LDI R17, 10
                                 DIVIDE_UNIDADES_LOOP:
0002a6 300a                          CPI R16, 10
0002a7 f010                          BRLO FIN_DIVIDE_UNIDADES				// Si R16 < 10, terminar
0002a8 1b01                          SUB R16, R17
0002a9 cffc                          RJMP DIVIDE_UNIDADES_LOOP
                                 FIN_DIVIDE_UNIDADES:
0002aa 9508                          RET
                                 
                                 DELAY:
                                     // Retardo para la multiplexacin
0002ab e302                          LDI R16, 50
                                 DELAY_LOOP:
0002ac 950a                          DEC R16
0002ad f7f1                          BRNE DELAY_LOOP
0002ae 9508                          RET
                                 
                                 DEBOUNCE:
0002af ef8f                          LDI R24, 255							// Retardo de debounce (ajusta este valor segn sea necesario)
                                 DEBOUNCE_LOOP:
0002b0 958a                          DEC R24
0002b1 f7f1                          BRNE DEBOUNCE_LOOP
0002b2 9508                          RET
                                 
                                 INICIO_TIMER0:
0002b3 e005                          LDI R16, (1 << CS02) | (1 << CS00)		// Configuramos el prescaler en 1024
0002b4 bd05                          OUT TCCR0B, R16
0002b5 e000                          LDI R16, 0								// Valor inicial del contador
0002b6 bd06                          OUT TCNT0, R16
0002b7 e001                          LDI R16, (1 << TOIE0)					// Habilitamos la interrupcin por desbordamiento
0002b8 9300 006e                     STS TIMSK0, R16
0002ba 9508                          RET
                                 
                                 TIMER0_ISR:
0002bb 930f                          PUSH R16								// Guardamos R16
0002bc b70f                          IN R16, SREG							// Guardamos el estado de SREG
0002bd 930f                          PUSH R16
                                 
0002be e000                          LDI R16, 0								// Colocamos el valor inicial del contador
0002bf bd06                          OUT TCNT0, R16
                                 
                                     // Incrementar el contador de interrupciones
0002c0 9100 0106                     LDS R16, CONTADOR_INTERRUPCIONES
0002c2 9503                          INC R16
0002c3 9300 0106                     STS CONTADOR_INTERRUPCIONES, R16
                                 
                                     // Verificar si han pasado 61 interrupciones (1 segundo)
0002c5 330d                          CPI R16, 61
0002c6 f479                          BRNE CHECK_LED_BLINK					// Si no es 61, verificar el parpadeo de LEDs
                                 
                                     // Reiniciar el contador de interrupciones
0002c7 e000                          LDI R16, 0
0002c8 9300 0106                     STS CONTADOR_INTERRUPCIONES, R16
                                 
                                     // Incrementar el contador de segundos
0002ca 9100 0107                     LDS R16, CONTADOR_SEGUNDOS
0002cc 9503                          INC R16
0002cd 9300 0107                     STS CONTADOR_SEGUNDOS, R16
                                 
                                     // Verificar si han pasado 60 segundos (1 minuto)
0002cf 330c                          CPI R16, 60
0002d0 f429                          BRNE CHECK_LED_BLINK					// Si no es 60, verificar el parpadeo de LEDs
                                 
                                     // Reiniciar el contador de segundos
0002d1 e000                          LDI R16, 0
0002d2 9300 0107                     STS CONTADOR_SEGUNDOS, R16
                                 
                                     // Incrementar el tiempo (minutos y horas)
0002d4 940e 02eb                     CALL INCREMENTAR_TIEMPO
                                 
                                 CHECK_LED_BLINK:
                                     // Verificar si han pasado 30 interrupciones (500ms)
0002d6 9100 0106                     LDS R16, CONTADOR_INTERRUPCIONES
0002d8 310e                          CPI R16, 30
0002d9 f469                          BRNE FIN_TIMER0							// Si no es 30, salir de la interrupcin
                                 
                                     // Alternar el estado de los LEDs
0002da 9100 0109                     LDS R16, LED_STATE
0002dc 3000                          CPI R16, 0
0002dd f029                          BREQ ENCENDER_LEDS
0002de 982c                          CBI PORTB, PB4							// Apagar LED en PB4
0002df e000                          LDI R16, 0
