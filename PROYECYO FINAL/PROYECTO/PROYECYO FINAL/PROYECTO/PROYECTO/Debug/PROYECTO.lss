                                     RET
                                 */
                                 
                                 COMPARAR_ALARMA:
                                 	// Verificar si la alarma est activa
000274 9100 010f                     LDS R16, ALARMA_ACTIVA
000276 3001                          CPI R16, 1
000277 f4e1                          BRNE ALARMA_NO_COINCIDE					// Si la alarma no est activa, salir
                                 
                                     // Comparar horas
000278 9100 0105                     LDS R16, DECENAS_HORAS_RAM
00027a 9110 010e                     LDS R17, ALARMA_DECENAS_HORAS_RAM
00027c 1701                          CP R16, R17
00027d f4b1                          BRNE ALARMA_NO_COINCIDE
                                 
00027e 9100 0104                     LDS R16, UNIDADES_HORAS_RAM
000280 9110 010d                     LDS R17, ALARMA_UNIDADES_HORAS_RAM
000282 1701                          CP R16, R17
000283 f481                          BRNE ALARMA_NO_COINCIDE
                                 
                                     // Comparar minutos
000284 9100 0103                     LDS R16, DECENAS_MINUTOS_RAM
000286 9110 010c                     LDS R17, ALARMA_DECENAS_MINUTOS_RAM
000288 1701                          CP R16, R17
000289 f451                          BRNE ALARMA_NO_COINCIDE
                                 
00028a 9100 0102                     LDS R16, UNIDADES_MINUTOS_RAM
00028c 9110 010b                     LDS R17, ALARMA_UNIDADES_MINUTOS_RAM
00028e 1701                          CP R16, R17
00028f f421                          BRNE ALARMA_NO_COINCIDE
                                 
                                     // Si coinciden, activar PD7
