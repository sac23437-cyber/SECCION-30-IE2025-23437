                                     LDI R16, 0                  ; Desactivar la alarma
                                     STS ALARMA_ACTIVA, R16
                                     RET
                                 */
                                 
                                 COMPARAR_ALARMA:
                                 	; Verificar si la alarma est activa
00029f 9100 010f                     LDS R16, ALARMA_ACTIVA
0002a1 3001                          CPI R16, 1
0002a2 f4e1                          BRNE ALARMA_NO_COINCIDE     ; Si la alarma no est activa, salir
                                 
                                     ; Comparar horas
0002a3 9100 0105                     LDS R16, DECENAS_HORAS_RAM
0002a5 9110 010e                     LDS R17, ALARMA_DECENAS_HORAS_RAM
0002a7 1701                          CP R16, R17
0002a8 f4b1                          BRNE ALARMA_NO_COINCIDE
                                 
0002a9 9100 0104                     LDS R16, UNIDADES_HORAS_RAM
0002ab 9110 010d                     LDS R17, ALARMA_UNIDADES_HORAS_RAM
0002ad 1701                          CP R16, R17
0002ae f481                          BRNE ALARMA_NO_COINCIDE
                                 
                                     ; Comparar minutos
0002af 9100 0103                     LDS R16, DECENAS_MINUTOS_RAM
0002b1 9110 010c                     LDS R17, ALARMA_DECENAS_MINUTOS_RAM
0002b3 1701                          CP R16, R17
0002b4 f451                          BRNE ALARMA_NO_COINCIDE
                                 
0002b5 9100 0102                     LDS R16, UNIDADES_MINUTOS_RAM
0002b7 9110 010b                     LDS R17, ALARMA_UNIDADES_MINUTOS_RAM
0002b9 1701                          CP R16, R17
0002ba f421                          BRNE ALARMA_NO_COINCIDE
                                 
