                                 MOSTRAR_DIGITO:
                                     ; Cargar el valor del dgito desde la tabla de 7 segmentos
0002c1 e0f0                          LDI ZH, HIGH(TABLA7SEG << 1)
0002c2 e4e4                          LDI ZL, LOW(TABLA7SEG << 1)
0002c3 0fe0                          ADD ZL, R16
0002c4 9104                          LPM R16, Z                  ; Obtenemos el valor de la tabla del display
0002c5 b90b                          OUT PORTD, R16              ; Mostramos R16 en el display
0002c6 9508                          RET
                                 
                                 DIVIDIR_DECENAS:
                                     ; Divide un nmero en decenas (R16 / 10)
0002c7 e01a                          LDI R17, 10
0002c8 2722                          CLR R18
                                 DIVIDE_LOOP:
0002c9 9523                          INC R18
0002ca 1b01                          SUB R16, R17
0002cb f7e8                          BRCC DIVIDE_LOOP            ; Si no hay acarreo, continuar
0002cc 952a                          DEC R18
0002cd 0f01                          ADD R16, R17
0002ce 2f02                          MOV R16, R18                ; Guardamos las decenas en R16
0002cf 9508                          RET
                                 
                                 DIVIDIR_UNIDADES:
                                     ; Obtiene las unidades de un nmero (R16 % 10)
0002d0 e01a                          LDI R17, 10
                                 DIVIDE_UNIDADES_LOOP:
0002d1 300a                          CPI R16, 10
0002d2 f010                          BRLO FIN_DIVIDE_UNIDADES    ; Si R16 < 10, terminar
0002d3 1b01                          SUB R16, R17
0002d4 cffc                          RJMP DIVIDE_UNIDADES_LOOP
                                 FIN_DIVIDE_UNIDADES:
0002d5 9508                          RET
                                 
                                 DELAY:
