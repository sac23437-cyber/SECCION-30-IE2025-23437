                                 DEBOUNCE_LOOP:
0002db 958a                          DEC R24
0002dc f7f1                          BRNE DEBOUNCE_LOOP
0002dd 9508                          RET
                                 
                                 INICIO_TIMER0:
0002de e005                          LDI R16, (1 << CS02) | (1 << CS00) ; Configuramos el prescaler en 1024
0002df bd05                          OUT TCCR0B, R16
0002e0 e000                          LDI R16, 0                  ; Valor inicial del contador
0002e1 bd06                          OUT TCNT0, R16
0002e2 e001                          LDI R16, (1 << TOIE0)       ; Habilitamos la interrupcin por desbordamiento
0002e3 9300 006e                     STS TIMSK0, R16
0002e5 9508                          RET
                                 
                                 TIMER0_ISR:
0002e6 930f                          PUSH R16                    ; Guardamos R16
0002e7 b70f                          IN R16, SREG                ; Guardamos el estado de SREG
0002e8 930f                          PUSH R16
                                 
0002e9 e000                          LDI R16, 0                  ; Colocamos el valor inicial del contador
0002ea bd06                          OUT TCNT0, R16
                                 
                                     ; Incrementar el contador de interrupciones
0002eb 9100 0106                     LDS R16, CONTADOR_INTERRUPCIONES
0002ed 9503                          INC R16
0002ee 9300 0106                     STS CONTADOR_INTERRUPCIONES, R16
                                 
                                     ; Verificar si han pasado 61 interrupciones (1 segundo)
