0001ac 9100 0101                     LDS R16, MES                ; Cargamos el mes desde RAM
0001ae 940e 02d0                     CALL DIVIDIR_UNIDADES       ; Separamos decenas y unidades
0001b0 2f60                          MOV R22, R16                ; Guardamos las unidades en R22
                                 
                                     ; Mostrar decenas del da
0001b1 2f03                          MOV R16, R19
0001b2 940e 02c1                     CALL MOSTRAR_DIGITO
0001b4 9a2b                          SBI PORTB, PB3              ; Activamos el display de decenas de da
0001b5 940e 02d6                     CALL DELAY
0001b7 982b                          CBI PORTB, PB3              ; Desactivamos el display
                                 
                                     ; Mostrar unidades del da
0001b8 2f04                          MOV R16, R20
0001b9 940e 02c1                     CALL MOSTRAR_DIGITO
0001bb 9a2a                          SBI PORTB, PB2              ; Activamos el display de unidades de da
0001bc 940e 02d6                     CALL DELAY
0001be 982a                          CBI PORTB, PB2              ; Desactivamos el display
                                 
                                     ; Mostrar decenas del mes
0001bf 2f05                          MOV R16, R21
0001c0 940e 02c1                     CALL MOSTRAR_DIGITO
0001c2 9a29                          SBI PORTB, PB1              ; Activamos el display de decenas de mes
0001c3 940e 02d6                     CALL DELAY
0001c5 9829                          CBI PORTB, PB1              ; Desactivamos el display
                                 
                                     ; Mostrar unidades del mes
0001c6 2f06                          MOV R16, R22
0001c7 940e 02c1                     CALL MOSTRAR_DIGITO
0001c9 9a28                          SBI PORTB, PB0              ; Activamos el display de unidades de mes
0001ca 940e 02d6                     CALL DELAY
0001cc 9828                          CBI PORTB, PB0              ; Desactivamos el display
                                 
                                     ; Encender el LED verde (PC0)
0001cd 982d                          CBI PORTB, PB5
0001ce 9a40                          SBI PORTC, PC0
0001cf 9841                          CBI PORTC, PC1
                                 
                                     ; Verificar el estado de los botones
0001d0 b106                          IN R16, PINC
0001d1 ff04                          SBRS R16, PC4               ; Si PC4 est presionado, incrementar la fecha
0001d2 940e 01d8                     CALL INCREMENTAR_FECHA_CONFIG
0001d4 ff05                          SBRS R16, PC5               ; Si PC5 est presionado, decrementar la fecha
0001d5 940e 01e2                     CALL DECREMENTAR_FECHA_CONFIG
0001d7 9508                          RET
                                 
                                 
                                 INCREMENTAR_FECHA_CONFIG:
0001d8 940e 02da                     CALL DEBOUNCE
                                     ; Incrementar el da
0001da 9100 0100                     LDS R16, DIA
0001dc 9503                          INC R16
0001dd 9300 0100                     STS DIA, R16
                                 
                                     ; Verificar si el da supera el mximo permitido para el mes actual
0001df 940e 01f0                     CALL VERIFICAR_DIA_MAXIMO
0001e1 9508                          RET
                                 
                                 DECREMENTAR_FECHA_CONFIG:
0001e2 940e 02da                     CALL DEBOUNCE
                                     ; Decrementar el da
0001e4 9100 0100                     LDS R16, DIA
0001e6 950a                          DEC R16
0001e7 9300 0100                     STS DIA, R16
                                 
                                     ; Verificar si el da es menor que 1
0001e9 3000                          CPI R16, 0
0001ea f421                          BRNE FIN_DECREMENTAR_FECHA_CONFIG
                                     ; Si es menor que 1, establecer el da al mximo permitido para el mes actual
0001eb 940e 0209                     CALL OBTENER_DIAS_MES
0001ed 9300 0100                     STS DIA, R16
                                 
                                 FIN_DECREMENTAR_FECHA_CONFIG:
0001ef 9508                          RET
                                 
                                 VERIFICAR_DIA_MAXIMO:
                                     ; Cargar el mes actual
0001f0 9100 0101                     LDS R16, MES
0001f2 950a                          DEC R16                     ; Ajustar el mes para usarlo como ndice (0-11)
                                 
                                     ; Cargar la cantidad de das del mes actual desde la tabla
0001f3 e0f0                          LDI ZH, HIGH(TABLADIAS << 1)
0001f4 e4ee                          LDI ZL, LOW(TABLADIAS << 1)
0001f5 0fe0                          ADD ZL, R16                 ; Sumar el ndice del mes
0001f6 9114                          LPM R17, Z                  ; Cargar los das del mes en R17
                                 
