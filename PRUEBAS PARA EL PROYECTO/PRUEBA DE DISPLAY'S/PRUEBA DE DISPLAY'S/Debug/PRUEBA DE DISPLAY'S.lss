0001f7 9100 0100                     LDS R16, DIA
0001f9 1701                          CP R16, R17
0001fa f068                          BRLO FIN_VERIFICAR_DIA_MAXIMO ; Si el da es menor que los das del mes, terminar
                                 
                                     ; Si el da es mayor que los das del mes, reiniciar el da a 1 e incrementar el mes
0001fb e001                          LDI R16, 1
0001fc 9300 0100                     STS DIA, R16
                                 
                                     ; Incrementar el mes
0001fe 9100 0101                     LDS R16, MES
000200 9503                          INC R16
000201 9300 0101                     STS MES, R16
                                 
                                     ; Verificar si el mes supera 12
000203 300d                          CPI R16, 13
000204 f018                          BRLO FIN_VERIFICAR_DIA_MAXIMO ; Si el mes es menor que 13, terminar
                                 
                                     ; Reiniciar el mes a 1 (enero)
000205 e001                          LDI R16, 1
000206 9300 0101                     STS MES, R16
                                 
